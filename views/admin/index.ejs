        <% if (routerName == 'dashboard') { %>
            Morris.Donut({
            element: 'morris-donut-activity',
            resize: true,
            colors: [
                '#ff8acc',
                '#5b69bc',
                '#35B8E0'
            ],
            data: [
                { label: "Lượt tạo bài viết", value: <%= activity.numCreatePost %> },
                { label: "Lượt đăng ký khóa học", value: <%= activity.numRegisterCourse %> },
                { label: "Lượt tạo khóa học", value: <%= activity.numCreateCourse %> }
            ]
        });
    
        Morris.Donut({
            element: 'morris-donut-quantity',
            resize: true,
            colors: [
                '#ff8acc',
                '#5b69bc',
                '#35B8E0'
            ],
            data: [
                { label: "Số lượng giáo viên", value: <%= account.numTeacher %> },
                { label: "Số lượng học viên", value: <%= account.numStudent %> },
                { label: "Số lượng khóa học", value: <%= account.numCourse %> }
            ]
        });
    
        Morris.Donut({
            element: 'morris-donut-course',
            resize: true,
            colors: [
                '#ff8acc',
                '#5b69bc',
                '#FF1744',
                '#35B8E0',
            ],
            data: [
                { label: "Khóa học đang hoạt động", value: <%= course.numCourseDoing %> },
                { label: "Khóa học hoàn thành", value: <%= course.numCourseComplete %> },
                { label: "Khóa học đã từ chối", value: <%= course.numCourseReject %> },
                { label: "Khóa học chờ phê duyệt", value: <%= course.numCoursePending %> }
            ]
        });
        <% } %>
        
