    <style>
        body {
            background-color: #ebeff2;
        }

        .col-course-id {
            max-width: 4%;
        }

        .col-course-name {
            max-width: 30%;
        }

        .col-course-start {
            max-width: 14%;
        }

        .col-course-end {
            max-width: 14%;
        }

        .col-course-category {
            max-width: 12%;
        }

        .col-course-fee {
            max-width: 12%;
        }

        .col-course-detail {
            max-width: 14%;
        }

        .card-body-action-course {
            display: flex;
            flex-direction: row;
            align-items: baseline;
            justify-content: space-between;
        }

        .card-body-action-right {
            display: flex;
            flex-direction: row;
            align-items: baseline;
        }

        .card-body-action-right .dropdown {
            margin-right: 10px;
        }

        .connectedSortable .img-fluid {
            background: gainsboro;
            height: fit-content;
            margin-right: 10px;
        }

        .info-teacher-account {
            margin-top: 16px;
        }

        .carousel-inner {
            height: 300px;
        }

        .accordion.custom-accordion .form-check-input:disabled {
            opacity: 1;
        }

        .accordion.custom-accordion .form-check-input:disabled~.form-check-label, .form-check-input[disabled]~.form-check-label {
            opacity: 1;
            color: #212529;
        }

        .file-container {
          display: grid;
          grid-template-columns: repeat(4, 1fr);
          gap: 10px;
        }
    
        .file-item {
          display: flex;
          align-items: center;
          margin-bottom: 10px;
          position: relative;
          border: 1px solid aqua;
          border-radius: 8px;
        }
    
        .file-item .delete-icon {
          position: absolute;
          width: 20px;
          height: 20px;
          right: 0;
          top: 5px;
          cursor: pointer;
        }
    
        .file-item img,
        .file-item video {
          width: 100%;
          height: auto;
          margin-right: 10px;
        }
    
        .file-item button {
          padding: 5px 10px;
          background-color: #ff0000;
          color: #fff;
          border: none;
          cursor: pointer;
        }
    
        .tab-bar {
          display: flex;
        }
    
        .tab {
          flex: 1;
          padding: 10px;
          background-color: #f0f0f0;
          cursor: pointer;
          max-width: 100px;
          margin-right: 2px;
        }
    
        .tab.active {
          background-color: #ccc;
        }
    
        .tab-content-course {
          display: none;
        }
    
        .tab-content-course.active {
          display: block;
        }
    
        .container-quiz {
          display: flex;
        }
    
        .left-column-quiz {
          width: 100px;
          background-color: #f2f2f2;
          display: flex;
          flex-direction: column;
        }
    
        .right-column-quiz {
          flex: 1;
          background-color: #fff;
        }
    
        .tab-header-quiz {
          list-style-type: none;
          margin: 0;
          padding: 0;
          width: 100%;
        }
    
        .tab-header-quiz li {
          padding: 10px;
          background-color: #ddd;
          cursor: pointer;
          margin-bottom: 2px;
        }
    
        .tab-header-quiz li.selected {
          background-color: #ccc;
        }
    
        .tab-content-course-quiz {
          display: none;
          padding: 20px;
          background-color: #f9f9f9;
        }
    
        .tab-content-course-quiz.selected {
          display: block;
        }
    
        .textarea-quiz {
          width: 70%;
          height: 120px;
          resize: none;
        }
    
        .quiz-answer {
          display: flex;
          margin-top: 10px;
          width: 70%;
        }
    
        .quiz-answer textarea {
          margin-left: 10px;
          resize: none;
          height: 50px;
          width: 100%;
        }
    
        .quiz-answer p {
          width: 15px;
        }
    
        .add-tab-quiz {
          width: 100%;
        }
    
        .loading-container {
          display: none;
          position: fixed;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          background-color: rgba(0, 0, 0, 0.5);
          z-index: 9999;
        }
    
        .loading-spinner {
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
        }
    
        .loading-spinner i {
          font-size: 40px;
          color: #3498db;
        }
    
        .dialog-container {
          display: none;
          position: fixed;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          background-color: rgba(0, 0, 0, 0.5);
          z-index: 9999;
        }
    
        .dialog-content {
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          background-color: #fff;
          box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
          padding: 20px;
          text-align: center;
          border-radius: 5px;
          display: flex;
          flex-direction: column;
        }
    
        .dialog-content span {
          font-size: 18px;
          margin-bottom: 20px;
        }
    
        .dialog-content button {
          background-color: #3498db;
          color: #fff;
          border: none;
          padding: 10px 20px;
          border-radius: 5px;
          cursor: pointer;
        }
        
        .vertical-line {
            width: 2px; /* Chiều rộng của đường kẻ */
            height: 500px; /* Chiều cao của đường kẻ */
            background-color: rgb(189, 186, 186); /* Màu sắc của đường kẻ */
            margin-left: 20px; /* Khoảng cách từ lề trái */
        }

        div.scrollmenu {
          overflow: auto;
          white-space: nowrap;
        }
    </style>
    
</head>

